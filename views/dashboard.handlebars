<h2>Dashboard</h2>
<h3>Your Posts</h3>
{{#each posts as |post|}}
        <section>
            <p><a href="/dashboard/edit/{{post.id}}">Edit Post</a></p>
            <header>{{post.title}}</header>
            <p>{{post.content}}</p>
            <p>Posted on {{format_date post.date_created}}</p>
        </section>
    {{/each}}
    <section>
        <button id="create-new">Create New Post</button>
    </section>

    <script src="/js/dashboard.js"></script>